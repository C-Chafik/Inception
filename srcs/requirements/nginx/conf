server {
	listen 80 default_server;
	listen [::]:80 default_server;

	listen 443 ssl http2 default_server;
	listen [::]:443 ssl http2 default_server;

	server_name cmarouf.42.fr;
	
	ssl_protocols TLSv1 TLSv1.2 TLSv1.3;
	ssl_certificate /etc/ssl/certs/cmarouf.42.fr.crt;
	ssl_certificate_key /etc/ssl/private/cmarouf.42.fr.key;

	location / {
		root /var/www/html/;
	}

}
